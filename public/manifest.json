{
    "manifest_version": 2,
    "name": "Trello Bookmark",
    "description": "Store your bookmarks as Trello Cards",
    "version": "2.4.2",
    "browser_action": {
        "default_popup": "index.html",
        "default_title": "Trello Bookmark"
    },
    "icons": [
        {
            "src": "icon128.png",
            "sizes": "128x128",
            "type": "image/png"
        }
    ],
    "permissions": [
        "tabs",
        "https://api.trello.com/",
        "<all_urls>"
    ],
    "web_accessible_resources": [
        "index.html"
    ]
}