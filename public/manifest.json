{
    "manifest_version": 3,
    "name": "Trello Bookmark",
    "description": "Store your bookmarks as Trello Cards",
    "version": "2.4.4",
    "action": {
        "default_popup": "index.html",
        "default_title": "Trello Bookmark"
    },
    "icons": [
        {
            "src": "icon128.png",
            "sizes": "128x128",
            "type": "image/png"
        }
    ],
    "permissions": [
        "tabs",
        "https://api.trello.com/",
        "<all_urls>"
    ],
    "web_accessible_resources": [
        {
            "resources": [
                "index.html"
            ],
            "matches": [
                "<all_urls>"
            ]
        }
    ]
}